@model InventoryManagement.Models.UserModel

@{
    ViewData["Title"] = "ViewUser";
}

<h1>ViewUser ID : @Html.DisplayFor(model => model.uidd)</h1>

<div>
    <hr />
    <dl class="row">
        
        <dt class = "col-sm-2">
            User Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.name)
        </dd>
        <dt class = "col-sm-2">
            User Phone No
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.phoneNo)
        </dd>
        <dt class = "col-sm-2">
           City
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.city)
        </dd>
        <dt class = "col-sm-2">
           Email ID
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.emailid)
        </dd>
    </dl>
